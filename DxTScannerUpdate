msg.Text = "PROF. SKID WAS HERE READY TO FUCK YOU" -- ENTER YOUR HACKING MSG
msg.Parent = workspace

task.delay(3, function() -- THIS TOO
    msg:Destroy() -- YOU CAN DELETE IT
end)]]
end)

